.familyLines
	figure
		max-width 50vw
		height 33vh
	.family__figure
		display grid
		columns 2
		grid-template-columns 75% 25%
		// grid-gap 10px
		.canvasContainer
			width 100%
			height 100%
		canvas
			margin-top 20px
		svg
			width 100%
			height 100%
		.line
			stroke #000
			stroke-width 1px
			fill none
			shape-rendering crispEdges
		.quintileLabel
			text-anchor end