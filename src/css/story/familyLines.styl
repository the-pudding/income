.familyLines
	figure
		max-width 600px
		height 33vh
	.family__figure
		display grid
		columns 2
		grid-template-columns 75% 25%
		// grid-gap 10px
		.canvasContainer
			width 100%
			height 100%
		svg
			width 100%
			height 100%
			text
				font-family $sans
		.line
			stroke #000
			stroke-width 1px
			fill none
			shape-rendering crispEdges
		.quintileLabel
			text-anchor end
		.label
			font-family $sans
			font-size 0.8em
			font-weight 700
			fill #828282